    "fs-jetpack": "^2.2.2",
    "lighthouse": "^5.1.0",
    "lodash": "^4.17.15",
    "minimist": "^1.2.0",
    "p-all": "^2.1.0",
    "puppeteer": "^1.19.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@types/debug": "^4.1.4",
    "@types/minimist": "^1.2.0",
    "@types/node": "^12.6.8",
    "@types/prettier": "^1.18.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  }
